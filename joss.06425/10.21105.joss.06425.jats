      <table>
        <thead>
          <tr>
            <th>Parameter</th>
            <th>30 FPS</th>
            <th>240 FPS</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td>Minimum Distance</td>
            <td>10 Frames</td>
            <td>50 Frames</td>
          </tr>
